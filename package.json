    "mongease-plugin",
    "mongease",
    "graphql",
    "schema"
  ],
  "dependencies": {
    "@types/lodash": "^4.14.54",
    "graphql": "^0.9.1",
    "graphql-tools": "^0.10.1",
    "lodash": "^4.17.4",
    "mongoose-to-graphql": "^1.0.9"
  },
  "devDependencies": {
